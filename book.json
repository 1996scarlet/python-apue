{
    "title": "UNIX环境中的全栈Python高级编程",
    "author": "1996scarlet <1996scarlet@gmail.com>",
    "language": "zh-hans",
    "plugins": [
        "github",
        "flexible-alerts",
        "expandable-chapters"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "github": {
            "url": "https://github.com/1996scarlet/python-apue"
        },
        "flexible-alerts": {
            "style": "callout",
            "1info": {
                "label": "提示",
                "icon": "fa fa-comments",
                "className": "info"
            },
            "1warn": {
                "label": "注意",
                "icon": "fa fa-warning",
                "className": "warning"
            }
        }
    }
}